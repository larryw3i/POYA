@model IEnumerable<POYA.Areas.LAdmin.Models.LAudit>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @(Html.DisplayNameFor(model => model.ContentId))
            </th>
            <th>
                @(Html.DisplayNameFor(model => model.DOAudit))
            </th>
            <th>
                @(Html.DisplayNameFor(model => model.InspectorId))
            </th>
            <th>
                @(Html.DisplayNameFor(model => model.AuditResultId))
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @(Html.DisplayFor(modelItem => item.ContentId))
            </td>
            <td>
                @(Html.DisplayFor(modelItem => item.DOAudit))
            </td>
            <td>
                @(Html.DisplayFor(modelItem => item.InspectorId))
            </td>
            <td>
                @(Html.DisplayFor(modelItem => item.AuditResultId))
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
